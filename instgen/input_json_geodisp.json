{
	"seed": 100,
	"network": "Chicago, Illinois",
	"problem": "DARP",
	"max_speed_factor": 0.5,
	"replicas": 1,
	"records": 100,
	"instance_filename": ["network", "problem", "records"],
	"parameters":[
		{
			"name": "min_early_departure",
			"type": "integer",
			"value": 0,
			"time_unit": "h"
		},
		{
			"name": "max_early_departure",
			"type": "integer",
			"value": 23,
			"time_unit": "h"
		},
		{
			"name": "min_distance",
			"type": "integer",
			"value": 500,
			"length_unit": "m"
		},
		{
			"name": "graphml",
			"type": "graphml",
			"value": true
		}
	],
	"attributes": [
		{
			"name": "earliest_departure",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"loc": 25200,
						"scale": 10800
					} ],
			"constraints": [ "earliest_departure >= 0", 
							"earliest_departure >= min_early_departure",
							 "earliest_departure <= max_early_departure"
						   ]
		},
		{
			"name": "time_window_length",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"loc": 600,
						"scale": 300
					} ],
			"output_csv": false
		},
		{
			"name": "latest_arrival",
			"type": "integer",
			"time_unit": "s",
			"expression": "earliest_departure + direct_travel_time + (direct_travel_time * 0.5)"
		},
		{
			"name": "lead_time",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"loc": 0,
						"scale": 300
					} ]
		},
		{  
			"name": "time_stamp",
			"type": "integer",
			"time_unit": "s",
			"expression": "earliest_departure - lead_time",
			"static_probability": 0.5
		},
		{  
			"name": "origin",
			"type": "location"
		},
		{  
			"name": "destination",
			"type": "location"
		},
		{
			"name": "direct_travel_time",
			"type": "integer",
			"time_unit": "s",
			"expression": "dtt(origin,destination)",
			"constraints": ["direct_travel_time >= 0"]
		},
		{
			"name": "direct_distance",
			"type": "integer",
			"length_unit": "m",
			"expression": "dist_drive(origin,destination)",
			"constraints": ["direct_distance >= min_distance"],
			"output_csv": false
		}
	],
	"travel_time_matrix": ["bus_stations"],
	"method_pois": [
	   {  
		   "locations": ["origin", "destination"],
		   "pdf": {
                    "type": "wald",
                    "loc": 34.028995255744775,
                    "scale": 7108.993523334802
			    	}
		}
	]
}