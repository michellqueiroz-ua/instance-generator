{
	"seed": [
		{
			"value": 100,
			"increment": 20
		}
	],
	"network": "Rennes, France",
	"problem": "DARP",
	"get_fixed_lines": "deconet",
	"request_demand_uniform": [
		{
			"min_time": 8,
			"max_time": 10,
			"number_of_requests": 500,
			"time_unit": "h"
		}
	],
	"spatial_distribution1": [
		{
			"num_origins": -1,
			"num_destinations": -1,
			"prob": 50,
			"is_random_origin_zones": true,
			"is_random_destination_zones": true
		}
	],
	"spatial_distribution2": [
		{
			"num_origins": -1,
			"num_destinations": -1,
			"prob": 50,
			"is_random_origin_zones": true,
			"is_random_destination_zones": true
		}
	],
	"spatial_distribution3": [
		{
			"num_origins": -1,
			"num_destinations": -1,
			"prob": 0
		}
	],
	"spatial_distribution4": [
		{
			"num_origins": -1,
			"num_destinations": -1,
			"prob": 0
		}
	],
	"planning_horizon": [
		{
			"min_early_departure": 5,
			"max_early_departure": 11,
			"time_unit": "h"
		}
	],
	"lead_time": [
		{
			"min_lead_time": 0,
			"max_lead_time": 5,
			"time_unit": "min"
		}
	],
	"walk_speed": [
		{
			"min_walk_speed": 5,
			"max_walk_speed": 5,
			"speed_unit": "kmh"
		}
	],
	"max_walking": [
		{
			"lb_max_walking": 600,
			"ub_max_walking": 600,
			"time_unit": "s"
		}
	],
	"number_replicas": 1,
	"delay_vehicle_factor": 0.5,
	"delay_walk_factor": 0.5,
	"time_window_length": [
		{
			"length": 10,
			"time_unit": "min"

		}
	],
	"request_demand_uniform": [
		{
			"type": "uniform",
			"min_time": 8,
			"max_time": 10,
			"number_of_requests": 100,
			"time_unit": "h"
		}
	],
	"zones": [
		{
			"name": "zone1",
			"lon": -1.6902891077472344,
			"lat": 48.09282962664396,
			"length_lon": 1000,
			"length_lat": 1000,
			"length_unit": "m" 
		},
		{
			"name": "zonecenter",
			"centroid": true,
			"radius": 1000
		},
		{
			"name": "zone2",
			"lon": -1.6945037524387636,
			"lat": 48.105993349477394,
			"radius": 1000,
			"length_unit": "m"
		}
	],
	"locations": [
		{
			"name": "depot1",
			"type": "coordinate",
			"lon": -1.6457340723441525,
			"lat": 48.100199454954804
		},
		{
			"name": "school1",
			"type": "school",
			"lon": -1.742000418660542,
			"lat": 48.09010529664507
		}
	],
	"parameters":[
		{
			"name": "min_early_departure",
			"type": "time",
			"value": 5,
			"time_unit": "h"
		},
		{
			"name": "max_early_departure",
			"type": "time",
			"value": 11,
			"time_unit": "h"
		},
		{
			"name": "depots",
			"type": "list_coordinates", 
			"size": 1,
			"list": ["depot1"],
			"locs": "anywhere"
		},
		{
			"name": "schools",
			"type": "list_coordinates", 
			"size": 2,
			"list": ["school1"],
			"locs": "schools"
		},
		{
			"name": "zonesorgn",
			"type": "list_zones", 
			"size": 3
		},
		{
			"name": "zonesdest",
			"type": "list_zones", 
			"size": 2,
			"list": ["zonecenter", "zone1"]
		},
		{
			"name": "travel_time_matrix",
			"type": "matrix",
			"value": true,
			"locations": ["bus_stations", "schools"]
		},
		{
			"name": "graphml",
			"type": "graphml",
			"value": true
		}
	],
	"attributes": [
		{
			"name": "earliest_departure",
			"type": "time",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"min": 8,
						"max": 10,
						"time_unit": "h"
					} ],
			"constraints": [ "earliest_departure >= 0", 
							"earliest_departure >= min_early_departure",
							 "earliest_departure <= max_early_departure"
						   ]
		},
		{
			"name": "latest_arrival",
			"type": "time",
			"time_unit": "s",
			"expression": "earliest_departure + direct_travel_time + (direct_travel_time * 0.5)"
		},
		{
			"name": "lead_time",
			"type": "time",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"min": 0,
						"max": 5,
						"time_unit": "min"
					} ],
			"static_probability": 0.5
		},
		{  
			"name": "time_stamp",
			"type": "time",
			"time_unit": "s",
			"expression": "earliest_departure - lead_time" 
		},
		{
			"name": "max_walking",
			"type": "time",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"min": 550,
						"max": 600,
						"time_unit": "s"
					} ],
			"output_csv": false
		},
		{
			"name": "walk_speed",
			"type": "speed",
			"speed_unit": "s",
			"pdf": [ {	"type": "uniform",
						"min": 5,
						"max": 5,
						"speed_unit": "kmh"
					} ],
			"output_csv": false
		},
		{  
			"name": "origin",
			"type": "coordinate",
			"subset_zones": "zonesorgn",
			"weights": [50, 30, 20]
		},
		{ 
			"name": "stops_orgn",
			"type": "list",
			"expression": "stops(origin)",
			"constraints": ["len(stops_orgn) > 0"]
		}, 
		{  
			"name": "destination",
			"type": "coordinate",
			"subset_zones": "zonesdest"
		},
		{ 
			"name": "stops_dest",
			"type": "list",
			"expression": "stops(destination)",
			"constraints": ["len(stops_dest) > 0", "not (set(stops_orgn) & set(stops_dest))"]
		},
		{
			"name": "school",
			"type": "coordinate", 
			"subset_locations": "schools"
		},
		{
			"name": "direct_travel_time",
			"type": "time",
			"time_unit": "s",
			"expression": "dtt(origin,destination)",
			"constraints": ["direct_travel_time >= 0"],
			"output_csv": false
		},
		{
			"name": "time_walking",
			"type": "time",
			"time_unit": "s",
			"expression": "walk(origin,destination)",
			"constraints": ["time_walking > max_walking"],
			"output_csv": false
		},
		{
			"name": "ambulatory",
			"type": "integer",
			"pdf": [ {	"type": "uniform",
						"min": 0,
						"max": 1
					} ],
			"weights": [90, 10]
		}
	]
}