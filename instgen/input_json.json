{
	"seed": 100,
	"network":"Rennes, France",
	"get_fixed_lines": "deconet",
	"replicas": 1,
	"records": 500,
	"zones": [
		{
			"name": "zonexx",
			"lon": -1.6902891077472344,
			"lat": 48.09282962664396,
			"length_lon": 1000,
			"length_lat": 1000,
			"length_unit": "m" 
		},
		{
			"name": "zonecenter",
			"centroid": true,
			"radius": 1000
		},
		{
			"name": "zone2x",
			"lon": -1.6945037524387636,
			"lat": 48.105993349477394,
			"radius": 1000,
			"length_unit": "m"
		}
	],
	"locations": [
		{
			"name": "depot1",
			"type": "coordinate",
			"lon": -1.6457340723441525,
			"lat": 48.100199454954804
		},
		{
			"name": "school1",
			"type": "school",
			"lon": -1.742000418660542,
			"lat": 48.09010529664507
		}
	],
	"parameters":[
		{
			"name": "problem",
			"type": "string",
			"value": "DARP"
		},
		{
			"name": "min_early_departure",
			"type": "time",
			"value": 5,
			"time_unit": "h"
		},
		{
			"name": "max_early_departure",
			"type": "time",
			"value": 11,
			"time_unit": "h"
		},
		{
			"name": "min_distance",
			"type": "length",
			"value": 2,
			"length_unit": "km"
		},
		{
			"name": "max_distance",
			"type": "length",
			"value": 5,
			"length_unit": "km"
		},
		{
			"name": "depots",
			"type": "list_coordinates", 
			"size": 1,
			"list": ["depot1"],
			"locs": "random"
		},
		{
			"name": "schools",
			"type": "list_coordinates", 
			"size": 2,
			"list": ["school1"],
			"locs": "schools"
		},
		{
			"name": "zonesorgn",
			"type": "list_zones", 
			"size": 3
		},
		{
			"name": "zonesdest",
			"type": "list_zones", 
			"size": 2,
			"list": ["zonecenter", "zonexx"]
		},
		{
			"name": "travel_time_matrix",
			"type": "matrix",
			"value": true,
			"locations": ["bus_stations"]
		},
		{
			"name": "graphml",
			"type": "graphml",
			"value": true
		}
	],
	"instance_filename": ["network", "problem", "number_records", "min_early_departure", "max_early_departure"],
	"attributes": [
		{
			"name": "earliest_departure",
			"type": "time",
			"time_unit": "s",
			"pdf": [ {	"type": "normal",
						"mean": 8.5,
						"std": 1,
						"time_unit": "h"
					} ],
			"constraints": [ "earliest_departure >= 0", 
							"earliest_departure >= min_early_departure",
							 "earliest_departure <= max_early_departure"
						   ]
		},
		{
			"name": "time_window_length",
			"type": "time",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"min": 10,
						"max": 15,
						"time_unit": "min"
					} ],
			"output_csv": false
		},
		{
			"name": "latest_departure",
			"type": "time",
			"time_unit": "s",
			"expression": "earliest_departure + time_window_length"
		},
		{
			"name": "earliest_departureP",
			"type": "time",
			"time_unit": "s",
			"pdf": [ {	"type": "poisson",
						"lam": 7,
						"time_unit": "h"
					} ],
			"constraints": [ "earliest_departureP >= 0", 
							"earliest_departureP >= min_early_departure",
							 "earliest_departureP <= max_early_departure"
						   ]
		},
		{
			"name": "earliest_arrival",
			"type": "time",
			"time_unit": "s",
			"expression": "latest_arrival - time_window_length"
		},
		{
			"name": "latest_arrival",
			"type": "time",
			"time_unit": "s",
			"expression": "earliest_departure + direct_travel_time + (direct_travel_time * 0.5)"
		},
		{
			"name": "lead_time",
			"type": "time",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"min": 0,
						"max": 5,
						"time_unit": "min"
					} ]
		},
		{  
			"name": "time_stamp",
			"type": "time",
			"time_unit": "s",
			"expression": "earliest_departure - lead_time",
			"static_probability": 0.5
		},
		{
			"name": "max_walking",
			"type": "time",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"min": 550,
						"max": 600,
						"time_unit": "s"
					} ],
			"output_csv": false
		},
		{
			"name": "walk_speed",
			"type": "speed",
			"speed_unit": "mps",
			"pdf": [ {	"type": "uniform",
						"min": 5,
						"max": 5,
						"speed_unit": "kmh"
					} ],
			"output_csv": false
		},
		{  
			"name": "origin",
			"type": "coordinate",
			"subset_zones": "zonesorgn",
			"weights": [50, 30, 20]
		},
		{ 
			"name": "stops_orgn",
			"type": "list",
			"expression": "stops(origin)",
			"constraints": ["len(stops_orgn) > 0"]
		}, 
		{  
			"name": "destination",
			"type": "coordinate",
			"subset_zones": "zonesdest"
		},
		{ 
			"name": "stops_dest",
			"type": "list",
			"expression": "stops(destination)",
			"constraints": ["len(stops_dest) > 0", "not (set(stops_orgn) & set(stops_dest))"]
		},
		{
			"name": "school",
			"type": "coordinate", 
			"subset_locations": "schools"
		},
		{
			"name": "direct_travel_time",
			"type": "time",
			"time_unit": "s",
			"expression": "dtt(origin,destination)",
			"constraints": ["direct_travel_time >= 0"],
			"output_csv": false
		},
		{
			"name": "direct_distance",
			"type": "length",
			"length_unit": "m",
			"expression": "dist_drive(origin,destination)",
			"constraints": ["direct_distance >= min_distance"],
			"output_csv": false
		},
		{
			"name": "time_walking",
			"type": "time",
			"time_unit": "s",
			"expression": "walk(origin,destination)",
			"constraints": ["time_walking > max_walking"],
			"output_csv": false
		},
		{
			"name": "ambulatory",
			"type": "integer",
			"pdf": [ {	"type": "uniform",
						"min": 0,
						"max": 1
					} ],
			"weights": ["randomized_weights"]
		},
		{
			"name": "accompanying",
			"type": "integer",
			"pdf": [ {	"type": "uniform",
						"min": 0,
						"max": 2
					} ],
			"weights": [70, 20, 10]
		},
		{
			"name": "inbound_outbound",
			"type": "integer",
			"pdf": [ {	"type": "uniform",
						"min": 0,
						"max": 1
					} ]
		}
	]
}