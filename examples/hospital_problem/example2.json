	"parameters":[
		{
			"name": "min_early_departure",
			"type": "integer",
			"value": 8,
			"time_unit": "h"
		},
		{
			"name": "max_early_departure",
			"type": "integer",
			"value": 18,
			"time_unit": "h"
		},
		{
			"name": "min_distance",
			"type": "integer",
			"value": 500,
			"length_unit": "m"
		},
		{
			"name": "graphml",
			"type": "graphml",
			"value": true
		},
		{
			"name": "hospitals",
			"type": "array_locations",
			"locs": "random",
			"size": 11,
			"value": ["hospital1", "hospital2", "hospital3", "hospital4", "hospital5", "hospital6", "hospital7", "hospital8", "hospital9", "hospital10", "hospital11"]
		}
	],
	"attributes": [
		{  
			"name": "origin",
			"type": "location"
		},
		{  
			"name": "destination",
			"type": "location",
			"subset_locations": "hospitals"
		},
		{
			"name": "pickup_from",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"loc": 25200,
						"scale": 36000
					} ],
			"constraints": [ "pickup_from >= 0", 
							"pickup_from >= min_early_departure",
							 "pickup_from <= max_early_departure"
						   ]
		},
		{
			"name": "pickup_to",
			"type": "integer",
			"time_unit": "s",
			"expression": "pickup_from + 1800"
		},
		{
			"name": "dropoff_from",
			"type": "integer",
			"time_unit": "s",
			"expression": "pickup_to + drivingDuration + 1800"
		},
		{
			"name": "dropoff_to",
			"type": "integer",
			"time_unit": "s",
			"expression": "dropoff_from + 1800"
		},
		{
			"name": "drivingDuration",
			"type": "integer",
			"time_unit": "s",
			"expression": "dtt(origin,destination)",
			"constraints": ["drivingDuration >= 0"]
		},
		{
			"name": "drivingDistance",
			"type": "integer",
			"length_unit": "m",
			"expression": "dist_drive(origin,destination)",
			"constraints": ["drivingDistance >= min_distance"]
		},
		{
			"name": "executionDuration",
			"type": "integer",
			"pdf": [ {	"type": "uniform",
						"loc": 420,
						"scale": 0
					} ]
		},
		{
			"name": "canBeCombined",
			"type": "integer",
			"pdf": [ {	"type": "uniform",
						"loc": 0,
						"scale": 1
					} ]
		},
		{
			"name": "maximumDetourRelative",
			"type": "real",
			"pdf": [ {	"type": "uniform",
						"loc": 0.25,
						"scale": 0
					} ]
		},
		{
			"name": "maximumDetourAbsolute",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"loc": 10000,
						"scale": 0
					} ]
		}
	],
	"travel_time_matrix": ["origin", "destination"]
}
