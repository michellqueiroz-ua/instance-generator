{
	"seed": 1,
	"network": "Chicago, Illinois",
	"problem": "ODBRP",
	"set_fixed_speed":
    {
        "vehicle_speed_data":20,
        "vehicle_speed_data_unit":"kmh"
    },
	"replicas": 1,
	"requests": 15000,
	"instance_filename": ["network", "problem", "requests"],
	"places":[
		{
			"name": "stadium",
			"type": "location",
			"lon": -87.655800,
			"lat": 41.948463
		}
	],
	"parameters":[
		{
			"name": "min_early_departure",
			"type": "float",
			"value": 11,
			"time_unit": "h"
		},
		{
			"name": "max_early_departure",
			"type": "float",
			"value": 18,
			"time_unit": "h"
		},
		{
			"name": "min_early_departure2",
			"type": "float",
			"value": 12,
			"time_unit": "h"
		},
		{
			"name": "max_early_departure2",
			"type": "float",
			"value": 13,
			"time_unit": "h"
		},
		{
			"name": "min_early_departure3",
			"type": "float",
			"value": 15,
			"time_unit": "h"
		},
		{
			"name": "max_early_departure3",
			"type": "float",
			"value": 16,
			"time_unit": "h"
		},
		{
			"name": "graphml",
			"type": "graphml",
			"value": true
		},
		{
			"name": "concertvenue",
			"type": "array_locations",
			"locs": "random",
			"size": 1,
			"value": ["stadium"]
		}
	],
	"attributes": [
		{  
			"name": "time_stamp",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"loc": 39600,
						"scale": 25200
					} ],
			"constraints": [ "time_stamp >= 0", 
							"time_stamp >= min_early_departure",
							 "time_stamp <= max_early_departure"
						   ]
		},
		{
			"name": "reaction_time",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"loc": 300,
						"scale": 0
					} ]
		},
		{
			"name": "earliest_departure",
			"type": "integer",
			"time_unit": "s",
			"expression": "time_stamp",
			"constraints": [ "earliest_departure >= 0", 
							"earliest_departure >= min_early_departure"
						   ]
		},
		{
			"name": "latest_departure",
			"type": "integer",
			"time_unit": "s",
			"expression": "time_stamp + reaction_time"
		},
		{
			"name": "latest_arrival",
			"type": "integer",
			"time_unit": "s",
			"expression": "earliest_departure + direct_travel_time + (reaction_time) + 3600"
		},
		{  
			"name": "time_stamp2",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "normal",
						"loc": 43200,
						"scale": 3600
					} ],
			"constraints": [ "time_stamp2 >= 0", 
							"time_stamp2 >= min_early_departure2",
							 "time_stamp2 <= max_early_departure2"
						   ]
		},
		{
			"name": "reaction_time2",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"loc": 300,
						"scale": 0
					} ]
		},
		{
			"name": "earliest_departure2",
			"type": "integer",
			"time_unit": "s",
			"expression": "time_stamp2",
			"constraints": [ "earliest_departure2 >= 0", 
							"earliest_departure2 >= min_early_departure2"
						   ]
		},
		{
			"name": "latest_departure2",
			"type": "integer",
			"time_unit": "s",
			"expression": "time_stamp2 + reaction_time2"
		},
		{
			"name": "latest_arrival2",
			"type": "integer",
			"time_unit": "s",
			"expression": "earliest_departure2 + direct_travel_time + (reaction_time2) + 3600"
		},
		{  
			"name": "time_stamp3",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "normal",
						"loc": 54000,
						"scale": 3600
					} ],
			"constraints": [ "time_stamp3 >= 0", 
							"time_stamp3 >= min_early_departure3",
							 "time_stamp3 <= max_early_departure3"
						   ]
		},
		{
			"name": "reaction_time3",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"loc": 300,
						"scale": 0
					} ]
		},
		{
			"name": "earliest_departure3",
			"type": "integer",
			"time_unit": "s",
			"expression": "time_stamp3",
			"constraints": [ "earliest_departure3 >= 0", 
							"earliest_departure3 >= min_early_departure3"
						   ]
		},
		{
			"name": "latest_departure3",
			"type": "integer",
			"time_unit": "s",
			"expression": "time_stamp3 + reaction_time3"
		},
		{
			"name": "latest_arrival3",
			"type": "integer",
			"time_unit": "s",
			"expression": "earliest_departure3 + direct_travel_time + (reaction_time3) + 3600"
		},
		{  
			"name": "origin",
			"type": "location",
			"subset_locations": "concertvenue"
		},
		{  
			"name": "destination",
			"type": "location"
		},
		{ 
			"name": "stops_orgn",
			"type": "array_primitives",
			"expression": "stops(origin)",
			"constraints": ["len(stops_orgn) > 0"]
		},
		{ 
			"name": "stops_dest",
			"type": "array_primitives",
			"expression": "stops(destination)",
			"constraints": ["len(stops_dest) > 0", "not (set(stops_orgn) & set(stops_dest))"]
		},
		{
			"name": "max_walking",
			"type": "integer",
			"time_unit": "s",
			"pdf": [ {	"type": "uniform",
						"loc": 550,
						"scale": 50
					} ],
			"output_csv": false
		},
		{
			"name": "walk_speed",
			"type": "real",
			"speed_unit": "mps",
			"pdf": [ {	"type": "uniform",
						"loc": 1.38889,
						"scale": 0
					} ],
			"output_csv": false
		},
		{
			"name": "direct_travel_time",
			"type": "integer",
			"time_unit": "s",
			"expression": "dtt(origin,destination)"
		},
		{
			"name": "direct_distance",
			"type": "integer",
			"length_unit": "m",
			"expression": "dist_drive(origin,destination)"
		}
	],
	"travel_time_matrix": ["bus_stations"]
}